{
  "version": 2,
  "buildCommand": "npm run optimize-images",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/Frontend/(.*)",
      "destination": "/Frontend/$1"
    },
    {
      "source": "/tour",
      "destination": "/Frontend/index.html"
    },
    {
      "source": "/queries",
      "destination": "/Frontend/queries.html"
    },
    {
      "source": "/transition",
      "destination": "/Frontend/transition.html"
    },
    {
      "source": "/placeholder",
      "destination": "/Frontend/placeholder.html"
    },
    {
      "source": "/welcome-flow",
      "destination": "/Frontend/welcome-flow.html"
    },
    {
      "source": "/",
      "destination": "/Frontend/welcome.html"
    }
  ],
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/Frontend/(.*\\.(js|jsx|mjs))",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}
